            }
            catch (e) {
                alert("加入收藏失败，请使用Ctrl+D进行添加");
            }
        }
    }

    var ok = '';
    $(function () {
        $('#search button').click(function () {
            if($('#search input[name="kw"]').val().length > 16){
                layer.msg("您搜索的关键词过长！",{
                    timer: 2500
                });
                return false;
            }
        });
        $(window).scroll(function () {
            if ($(window).scrollTop() > 500) {
                $(".toTop").fadeIn(1000);
            }
            else {
                $(".toTop").fadeOut(1000);
            }
        });
    });


    $(window).scroll(function(){

        var _scroll = $(window).scrollTop();

        if(_scroll>130){
            $(".floatNav").fadeIn();
        }else{
            $(".floatNav").fadeOut();
        }

    })

</script>

